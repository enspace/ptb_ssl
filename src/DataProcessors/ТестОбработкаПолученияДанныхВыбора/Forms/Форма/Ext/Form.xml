			<CheckBoxType>Auto</CheckBoxType>
			<ContextMenu name="РежимОтладкиКонтекстноеМеню" id="8"/>
			<ExtendedTooltip name="РежимОтладкиРасширеннаяПодсказка" id="9"/>
